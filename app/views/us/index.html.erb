<p id="notice"><%= notice %></p>

<h1>Us</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Age</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @us.each do |u| %>
      <tr>
        <td><%= u.name %></td>
        <td><%= u.age %></td>
        <td><%= link_to 'Show', u %></td>
        <td><%= link_to 'Edit', edit_u_path(u) %></td>
        <td><%= link_to 'Destroy', u, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New U', new_u_path %>
